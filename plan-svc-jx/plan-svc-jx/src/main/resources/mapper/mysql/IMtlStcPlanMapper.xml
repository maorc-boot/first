<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asiainfo.biapp.pec.plan.jx.link.dao.IMtlStcPlanDao">

    <select id="queryListBySql" parameterType="string" resultType="map">
        SELECT DISTINCT A.PLAN_ID,
        PLAN_NAME
        FROM MCD_PLAN_DEF A
        where STATUS = 1
        limit 0,1000000
    </select>

</mapper>
