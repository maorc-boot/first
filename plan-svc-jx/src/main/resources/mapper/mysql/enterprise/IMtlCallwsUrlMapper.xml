<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asiainfo.biapp.pec.plan.jx.enterprise.mapper.IMtlCallwsUrlMapper">

    
    <select id="findByCond" resultType="com.asiainfo.biapp.pec.plan.jx.enterprise.model.McdSysInterfaceDef">
        select * from mcd_sys_interface_def  a
        where 1=1
          and a.callws_url_code = #{callwsUrlCode}
    </select>
</mapper>